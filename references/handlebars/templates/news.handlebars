{{#each news}}
    <div class="story">
        <h3>{{title}}</h3>
        <p class="timestamp">{{timestamp}}</p>
        <div class="post">
            <iframe src={{video}} frameborder="0" 
            allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            <p class="description">{{description}}</p>
        </div>
    </div>
{{/each}}